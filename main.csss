}
main ul {
  list-style: none;
}
main .group {
  display: flex;
  margin-bottom: "";
}
main .group .date {
  width: 140px;
  font-weight: 500;
}
main .group .info {
  width: calc(100% - 65px);
  padding-left: 25px;
  position: relative;
}
main .group .info .info_title {
  text-transform: uppercase;
  font-weight: 800;
  letter-spacing: 4px;
}
main .group .info .info_com {
  font-style: oblique;
  letter-spacing: 3px;
}
main .group .info :before {
  content: "";
  position: absolute;
  top: 3px;
  left: 0;
  width: 10px;
  height: 10px;
  background: brown;
  border-radius: 50%;
}
main .group .info :after {
  content: "";
  position: absolute;
  top: 10px;
  left: 4px;
  width: 2px;
  height: 90%;
  background: brown;
}

