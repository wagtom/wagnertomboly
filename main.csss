footer {
  background: white;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 0.8em;
}

@media only screen and (min-width: 500px) and (max-width: 700px) {
  html body .corpo .corpo_central {
    display: block;
    margin: 0;
    padding: 0;
  }
  html body .corpo .corpo_central main {
    width: 100%;
  }
  html body .corpo .corpo_central aside {
    width: 100%;
  }
}
@media only screen and (max-width: 500px) {
  html body .corpo .corpo_central {
    display: block;
    margin: 0;
    padding: 0;
  }
  html body .corpo .corpo_central main {
    width: 100%;
  }
  html body .corpo .corpo_central main .main_group .group {
    display: block;
  }
  html body .corpo .corpo_central aside {
    width: 100%;
  }
  html body .corpo .corpo_central aside .main_group .group {
    display: block;
  }
}

